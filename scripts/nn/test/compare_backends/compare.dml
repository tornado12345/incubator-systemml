X = read($1)
Y = read($2)
msg = ifdef($3, " ")
eps = 1e-6
# Normalize X and Y
X = X / max(X)
Y = Y / max(Y)
num_mismatch = sum(abs(X - Y) > eps)
if(num_mismatch > 0) {
	print("---------------------------------------------------\nERROR: >>>>>>>>> The results don't match(num_mismatch:" + num_mismatch + "): " + msg + "\n---------------------------------------------------")
	Z = abs(X - Y) > eps
	print("X=" + toString(X*Z))
	print("Y=" + toString(Y*Z))
	
}
else {
	print("The results match: " + msg)
}
