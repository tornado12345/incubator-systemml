foo1 = function (Matrix[Double] X)
    return (Matrix[Double] Y)
{  
   s = sum(X);
   if( s > 0 )
      print(ncol(X)+" cols -> "+s); 
   for( i in 1:10 ) {
      batch = ncol(X)/10;
      tmp = X[,((i-1)*batch+1):(i*batch)];      
      tmp = foo2(tmp, batch);
   }
   Y = X*2;  
}

foo2 = function (Matrix[Double] X, Integer batch)
    return (Matrix[Double] Y)
{  
   if( batch > 1 )
      Y = foo3(X);
   else
      Y = X;   
}

foo3 = function (Matrix[Double] X)
    return (Matrix[Double] Y)
{  
   if( sum(X) >= 0 )
      Y = foo1(X);
   else
      Y = X;      
}

V = read($1);
V = foo3(V); 
print(sum(V));
