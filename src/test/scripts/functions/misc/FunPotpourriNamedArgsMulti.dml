foo = function(Matrix[Double] A, Matrix[Double] B) return (Matrix[Double] C, Matrix[Double] D){
  while(FALSE){} # no inlining
  C = A %*% B + 7;
  D = C + 3;
}

X1 = matrix(1, 300, 500)
X2 = matrix(2, 500, 200)

if( $1 )
  [C,D] = foo(A=X2, B=X1);
else
  [C,D] = foo(B=X2, A=X1);

print("out:  " + sum(C))
