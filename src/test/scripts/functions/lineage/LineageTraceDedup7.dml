X = rand(rows=10, cols=5, seed=42);
R = X + 1;

for(i in 1:8) {
  if( i %% 2 == 0 ) {
	  R = (R + 1) / 2
	  if( i %% 4 == 0 )
	    R = (R + 1) + R
	}
	else {
		R = t(R) %*% R;
	} 
	
	if( i %% 3 == 0 )
    R = R / 5
  else 
    R = ((R + 1) - R/2) * 3 
}

R = R * 3;

write(R, $1);
